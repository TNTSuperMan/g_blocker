{
    "manifest_version": 3,
    "name": "g_blocker",
    "version": "2.9.1",
    "description": "Block google's watching and networking",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "main.js"
    },
    "declarative_net_request": {
        "rule_resources": [{
            "enabled": true,
            "id": "block",
            "path": "block.json"
        }]
    }
}