{
    "manifest_version": 3,
    "name": "g_blocker",
    "version": "2.9.1",
    "description": "Block google's watching and networking",
    "permissions": [
        "declarativeNetRequest",
        "webRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "main.js"
    }
}